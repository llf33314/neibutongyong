<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gt.inside.core.dao.dict.DictDAO">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.gt.inside.core.entity.dict.Dict">
		<id column="id" property="id" />
		<result column="dict_code" property="dictCode" />
		<result column="dict_name" property="dictName" />
		<result column="dict_remark" property="dictRemark" />
		<result column="create_time" property="createTime" />
		<result column="delete_flag" property="deleteFlag" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, dict_code, dict_name, dict_remark, create_time, delete_flag
    </sql>

</mapper>
